    //     return () => {\n            \n    //     };\n    // }, []);\n\n    return (\n        <div>\n            {/* <h1>Do I Dare Disturb the Universe?</h1> */}\n            <div class=\"title-foto\"></div>\n            <div className=\"landing\">\n                <h1 id = \"name\"> Hannah Gross </h1>\n            </div>\n        </div>\n    );\n}\n","/Users/manuela/Desktop/personal_website/src/components/Experience.js",[],"/Users/manuela/Desktop/personal_website/src/components/Education.js",["70"],"import './../App.css';\nimport brown_logo from \"./../brown-logo.png\"\n\nexport default function Education() {\n    return (\n        <div id=\"education-section\">\n\n            <h2 className=\"section-heading\" > Education </h2>\n\n            <div className=\"education-card\" >\n                <div className=\"section-text\">\n                    <h3> Brown University <img style={{ width: \"5%\", paddingLeft: \"1%\" }} src={brown_logo}></img> </h3>\n\n                    <p>\n                        The following are my favorite classes that I've taken so far. <br />\n                        <br />\n                        In Computer Science:\n                        <ul>\n                            <li> Computer Networks </li>\n                            <li> Design and Implementation of Programming Languages </li>\n                            <li> Systems Security (w/ extra lab component) </li>\n                            <li> Introduction to Computer Systems </li>\n                        </ul>\n\n                        In Mathematics:\n                        <ul>\n                            <li> Cryptography </li>\n                            <li> Topology </li>\n                            <li> Graph Theory </li>\n                        </ul>\n\n                        In other departments:\n                        <ul>\n                            <li> Philosophy of Attention </li>\n                            <li> Russia: Reforms to World Wars </li>\n                            <li> Topics in Constitutional Law </li>\n                            <li> Cyber Security: Strategy and Policy </li>\n                        </ul>\n                    </p>\n                </div>\n            </div>\n\n        </div>\n    )\n\n}","/Users/manuela/Desktop/personal_website/src/components/LoadingPage.js",[],"/Users/manuela/Desktop/personal_website/src/components/Footer.js",["71","72","73"],"import './../App.css';\nimport email_logo from './../email-logo.png'\nimport github_logo from './../github-logo.png'\nimport linkedin_logo from './../linkedin-logo.png'\n\n\nexport default function Footer() {\n    return(\n        <div id=\"footer\">\n            <div id=\"footer-contact-info\"> \n                <a className=\"contact-info\" href='mailto:hannah_gross@brown.edu'> \n                    <img src={email_logo} style={{width: \"3vw\",}}/>\n                </a>\n                <a className=\"contact-info\" href=\"https://github.com/hannahmanuela\"> \n                    <img src={github_logo} style={{width: \"4vw\",}}/>\n                </a>\n                <a className=\"contact-info\" href=\"https://www.linkedin.com/in/hannah-mn-gross/\"> \n                    <img src={linkedin_logo} style={{width: \"2vw\",}}/>\n                </a>\n            </div>\n            <div>\n                <span> Site designed and made by Hannah Gross </span>\n            </div>\n        </div>\n    )\n}","/Users/manuela/Desktop/personal_website/src/components/Experience-Professional.js",["74","75","76"],"import './../App.css';\nimport theator_summer from \"./../theator.jpg\"\nimport vaha_summer from \"./../vaha_2020.png\"\nimport amazon_summer from \"./../amazon-photo.jpg\"\n\n\nexport default function ExperienceProfessional() {\n    return (\n        <div className=\"experience-section\" id=\"professional-experience-section\">\n            <h2 className=\"section-heading\" > Experience - Professional </h2>\n\n            <div className=\"experience-card\" >\n                <div className=\"section-text\">\n                    <h3> Amazon Internship 2022 </h3>\n                    <p>\n                        I spent the summer of 2022 working in Seattle at Amazon (within AWS). <br />\n                        I worked on the <span style={{ fontWeight: \"bold\" }}> Identity team </span>, whose job was to store and manage passwords and password policies for AWS <br />\n                        <br />\n                        For the internship project, I integrated <span style={{ fontWeight: \"bold\" }}> checks for weak passwords and compromised logins </span> into their existing infrastructure of password management and policies.  <br />\n                        Over the summer, I designed and implemented these changes, created a functioning prototype, and made a final presentation to the larger team.\n                    </p>\n                </div>\n                <img className=\"section-img\" style={{ width: \"25%\", }} src={amazon_summer}></img>\n            </div>\n\n            <div className=\"experience-card\" >\n                <div className=\"section-text\">\n                    <h3> Theator Internship 2021 </h3>\n                    <p>\n                        I spent the second COVID-summer of 2021 working for a Tel Aviv based company called Theator. <b />\n                        I had worked with them previously during my gap year. In this summer I ran and implemented a project to\n                        set up an <span style={{ fontWeight: \"bold\" }}> end to end data infrastructure </span>. <br />\n                        This included\n                        <ul>\n                            <li> building a <span style={{ fontWeight: \"bold\" }}> custom coded ETL process </span> from their data lake in elasticsearch to a relational database in Redshift. </li>\n                            <li> building a <span style={{ fontWeight: \"bold\" }}> full data warehouse </span> on AWS </li>\n                            <li> creating a <span style={{ fontWeight: \"bold\" }}> backend API </span> for that data warehouse </li>\n                            <li> and designing <span style={{ fontWeight: \"bold\" }}> dashboards </span> on Metabase </li>\n                        </ul>\n                    </p>\n                </div>\n                <img className=\"section-img\" style={{ width: \"30%\", }} src={theator_summer}></img>\n            </div>\n\n            <div className=\"experience-card\" >\n                <div className=\"section-text\">\n                    <h3> Vaha Internship 2020 </h3>\n                    <p>\n                        I spent the COVID-summer of 2020 working for a Berlin based company called Vaha. <b />\n                        I worked closely with an outside consultant to <span style={{ fontWeight: \"bold\" }}> build from scratch a full data warehouse on Postgresql. </span>  <br />\n                        This included coming up with <span style={{ fontWeight: \"bold\" }}> customer attribution models and ratings tracking. </span>  <br />\n                        I also, using that data warehouse, put together <span style={{ fontWeight: \"bold\" }}> dashboards and views for managers and shareholders. </span>\n                    </p>\n                </div>\n                <img className=\"section-img\" style={{ width: \"30%\", }} src={vaha_summer}></img>\n            </div>\n\n        </div>\n    )\n\n}","/Users/manuela/Desktop/personal_website/src/components/ResearchExperience.js",["77"],"import './../App.css';\nimport ednaFig from \"./../edna-fig.png\"\n\n\n\nexport default function ResearchExperience() {\n    return (\n        <div className=\"experience-section\" id=\"research-experience-section\">\n            <h2 className=\"section-heading\" > Research Experience </h2>\n\n            <div className=\"experience-card\" >\n                <div className=\"section-text\">\n                    <h3> Funhouse </h3>\n                    <p>\n                        Today, many companies have databases that contain sensitive customer data, and their employees need access to that\n                        data to do their jobs. However, which data they need, and at what fidelity, changes over time -- for example a\n                        customer service representative who is on the phone with a customer needs access to that customer's information,\n                        but not others'. Because changing permissions on-the-fly is difficult, employees generally have more access than\n                        they actually need at that moment. <br />\n                        <br />\n                        Funhouse is a new kind of database that fundamentally shows different users different versions of the information\n                        it contains. It allows for an anonymized baseline from which users can easily and temporarily upgrade their access.\n                        When a user queries Funhouse, she is actually accessing a mirror of the database, an anonymized version of the\n                        database that is determined by her user group (e.g.backend dev).A mirror is defined by a set of anonymizations on\n                        top of the ground truth database. For example, a mirror might have anonymizations that change users' names,\n                        decorrelate a user's bookings from the user, and hide sensitive details such as their credit card\n                        information. <br />\n                        In order to provide the desired flexibility in permissions, Funhouse supports upgrades, where an individual\n                        employee can call on Funhouse to temporarily no longer anonymize certain ground truth data (e.g. a customer service\n                        representative can get an upgrade for relevant data related to a specific customer). These upgrades temporarily\n                        change the definition of the user's mirror, i.e. she temporarily operates on her own mirror which diverges from\n                        her user group's base mirror. Upgrades are audit-logged and require cryptographic material from the affected\n                        customer or someone with equivalent access. <br />\n                    </p>\n                </div>\n            </div>\n\n            <div className=\"experience-card\" >\n                <div className=\"section-text\">\n                    <h3> Edna </h3>\n                    <p>\n                        Edna, helps web applications provide users with more control over their data. Currently, web services store user\n                        data indefinitely, including sensitive information. This can expose the data to other users of the web service,\n                        as well as to attackers or insiders via common web vulnerabilities like SQL injections or compromise of privileged\n                        accounts. Edna is a library that helps web applications implement sealing, which removes or dissociates some or\n                        all of a user's data, and revealing, which restores the sealed data at a user's request, without breaking the\n                        application for other users. Sealing transformations store user data in an encrypted form and make any necessary\n                        changes to the application database. Revealing reintroduces the sealed data to the application database. <br />\n                        <br />\n                        A piece I contributed to Edna was a better story for key management that avoids depending on end users to safely\n                        store a private key. I designed an authentication scheme that allows users to authenticate (i.e. give the application\n                        enough information to regenerate the private key) with either their application password or a share of their private\n                        key. To realize this schema, I created a variant of Shamir's Secret Sharing with one share based on the user's\n                        password (via a PBKDF) and integrated the new authentication workflow into Edna.\n                    </p>\n                </div>\n                <img className=\"section-img\" style={{ width: \"30%\", }} src={ednaFig}></img>\n            </div>\n\n        </div>\n    )\n}\n\n",{"ruleId":"78","replacedBy":"79"},{"ruleId":"80","replacedBy":"81"},{"ruleId":"82","severity":1,"message":"83","line":9,"column":8,"nodeType":"84","messageId":"85","endLine":9,"endColumn":11},{"ruleId":"86","severity":1,"message":"87","line":39,"column":39,"nodeType":"88","messageId":"89","endLine":39,"endColumn":113,"fix":"90"},{"ruleId":"91","severity":1,"message":"92","line":44,"column":29,"nodeType":"88","endLine":44,"endColumn":79},{"ruleId":"91","severity":1,"message":"92","line":47,"column":29,"nodeType":"88","endLine":47,"endColumn":80},{"ruleId":"91","severity":1,"message":"92","line":50,"column":29,"nodeType":"88","endLine":50,"endColumn":82},{"ruleId":"91","severity":1,"message":"92","line":55,"column":13,"nodeType":"88","endLine":55,"endColumn":77},{"ruleId":"82","severity":1,"message":"93","line":2,"column":17,"nodeType":"84","messageId":"85","endLine":2,"endColumn":26},{"ruleId":"91","severity":1,"message":"92","line":12,"column":43,"nodeType":"88","endLine":12,"endColumn":108},{"ruleId":"91","severity":1,"message":"92","line":12,"column":21,"nodeType":"88","endLine":12,"endColumn":68},{"ruleId":"91","severity":1,"message":"92","line":15,"column":21,"nodeType":"88","endLine":15,"endColumn":69},{"ruleId":"91","severity":1,"message":"92","line":18,"column":21,"nodeType":"88","endLine":18,"endColumn":71},{"ruleId":"91","severity":1,"message":"92","line":23,"column":17,"nodeType":"88","endLine":23,"endColumn":92},{"ruleId":"91","severity":1,"message":"92","line":42,"column":17,"nodeType":"88","endLine":42,"endColumn":93},{"ruleId":"91","severity":1,"message":"92","line":55,"